	<queryString>
		<![CDATA[select tran_date,tran_type,sum(amount) amount,sum(amount1) amount1,TRAN_OPTION,TRAN_OPTION1,desp
from (select concat('PHARMACY(',loc.location_name,')') AS tran_type, date(sh.sale_date) tran_date, sum(sh.sale_amount) amount, 0 as amount1,
			 'Total Expenses : ' AS TRAN_OPTION, 1 AS TRAN_OPTION1, sh.sale_inv_id desp
		from v_sale sh,location loc
	   where sh.deleted = false and sh.admission_no = $P{adm_no}
		 and sh.location_id = loc.location_id and date(sh.sale_date) between date($P{adm_date}) and date($P{tran_date})
	   group by date(sh.sale_date),loc.location_name, sh.sale_inv_id
	   union all
	  select og.group_name as tran_type, date(vo.opd_date) tran_date, sum(vo.amount) amount, 0 as amount1,
			 'Total Expenses : ' AS TRAN_OPTION, 1 AS TRAN_OPTION1, vo.service_name as desp
		from v_opd vo, opd_group og
	   where vo.group_id = og.group_id and deleted = false and admission_no = $P{adm_no}
		 and date(vo.opd_date) between date($P{adm_date}) and date($P{tran_date})
	   group by og.group_name, date(vo.opd_date), vo.service_name
	   union all
	  select 'DC' as tran_type, date(dc_date) tran_date, sum(amount) amount, 0 as amount1,
			 'Total Expenses : ' AS TRAN_OPTION, 1 AS TRAN_OPTION1, service_name as desp
	    from v_dc
	   where deleted = false and admission_no = $P{adm_no}
		 and v_dc.service_id not in (select service_id from dr_detail_id where option = 'DC')
		 and date(dc_date) between date($P{adm_date}) and date($P{tran_date})
		 and v_dc.service_id not in (select sys_prop_value from sys_prop
								    where sys_prop_desp in ('system.dc.deposite.id','system.dc.disc.id','system.dc.paid.id',
                                    'system.dc.refund.id'))
	   group by date(dc_date), service_name
	   union all
	  select 'DC' as tran_type,date(dc_date) tran_date, sum(amount) amount, 0 as amount1,
			 'Total Expenses : ' AS TRAN_OPTION, 1 AS TRAN_OPTION1, service_name as desp
		from v_dc ,dc_doctor_fee ddf,doctor d
	   where deleted = false and admission_no = $P{adm_no} and v_dc.dc_detail_id = ddf.dc_detail_id
		 and ddf.doctor_id = d.doctor_id and date(dc_date) between date($P{adm_date}) and date($P{tran_date})
		 and v_dc.service_id not in (select sys_prop_value from sys_prop
								    where sys_prop_desp in ('system.dc.deposite.id','system.dc.disc.id','system.dc.paid.id',
                                    'system.dc.refund.id'))
	   group by date(dc_date), service_name
	   union all
	  select 'OT' as tran_type, date(ot_date) tran_date, sum(amount) amount, 0 as amount1,
			 'Total Expenses : ' AS TRAN_OPTION, 1 AS TRAN_OPTION1, service_name as desp
		from v_ot
	   where deleted = false and admission_no = $P{adm_no}
		 and v_ot.service_id not in (select service_id from dr_detail_id where option = 'OT')
		 and date(ot_date) between date($P{adm_date}) and date($P{tran_date})
		 and v_ot.service_id not in (select sys_prop_value from sys_prop
								    where sys_prop_desp in ('system.ot.deposite.id','system.ot.disc.id','system.ot.paid.id',
                                    'system.ot.refund.id'))
	   group by date(ot_date), service_name
	   union all
	  select 'OT' as tran_type, date(ot_date) tran_date, sum(amount) amount, 0 as amount1,
			 'Total Expenses : ' AS TRAN_OPTION, 1 AS TRAN_OPTION1, service_name as desp
		from v_ot,ot_doctor_fee odf,doctor d
	   where deleted = false and admission_no = $P{adm_no} and v_ot.ot_detail_id = odf.ot_detail_id
		 and odf.doctor_id = d.doctor_id and date(ot_date) between date($P{adm_date}) and date($P{tran_date})
		 and v_ot.service_id not in (select sys_prop_value from sys_prop
								    where sys_prop_desp in ('system.ot.deposite.id','system.ot.disc.id','system.ot.paid.id',
                                    'system.ot.refund.id'))
	   group by date(ot_date), service_name
	   union all
	  select 'RETURN IN ' as tran_type, date(ret_in_date) tran_date, 0 as amount, sum(ret_in_amount*-1) amount1,
		     'Total Expenses : ' AS TRAN_OPTION, 1 AS TRAN_OPTION1, ret_in_id as desp
		from v_return_in
	   where deleted = false and admission_no = $P{adm_no}
		 and date(ret_in_date) between date($P{adm_date}) and date($P{tran_date})
	   group by date(ret_in_date), ret_in_id
	   union all
	  select tran_type, tran_date, 0 as amount, sum(ifnull(amount,0))*-1 amount1, TRAN_OPTION, 2 AS TRAN_OPTION1, desp
		from (select 'Payment' as tran_type, dt.tran_date, dt.desp, 1 as qty,
					case dt.desp
						when 'Pharmacy Discount' then dt.discount
						when 'Pharmacy Paid' then dt.paid
						when 'Pharmacy Tax' then dt.tax
						else 0
					end as amount, 'Total Payment : ' as TRAN_OPTION
				from (select a.*, b.*
						from (select date(sale_date) as tran_date, sum(ifnull(discount,0)) as discount, 
									 sum(ifnull(paid_amount,0)) as paid, sum(ifnull(tax_amt,0)) as tax
								from sale_his where deleted = false and admission_no = $P{adm_no}
								 and date(sale_date) between date($P{adm_date}) and date($P{tran_date})
							   group by date(Sale_date)) a,
							 (select 'Pharmacy Discount' as desp
							   union all 
							  select 'Pharmacy Paid' as desp
							   union all
							  select 'Pharmacy Tax' as desp) b) dt
				union all
				select 'Payment' as tran_type, dt.tran_date, dt.desp, 1 as qty,
					case dt.desp
						when 'OPD Discount' then dt.discount
						when 'OPD Paid' then dt.paid
						when 'OPD Tax' then dt.tax
						else 0
					end as amount, 'Total Payment : ' as TRAN_OPTION
				from (select a.*, b.*
						from (select date(opd_date) as tran_date, sum(ifnull(disc_a,0)) as discount , 
									 sum(ifnull(paid,0)) as paid, sum(ifnull(tax_a,0)) as tax
								from opd_his where deleted = false and admission_no = $P{adm_no}
								 and date(opd_date) between date($P{adm_date}) and date($P{tran_date})
							   group by date(opd_date)) a,
							 (select 'OPD Discount' as desp
							   union all 
							  select 'OPD Paid' as desp
							   union all
							  select 'OPD Tax' as desp) b) dt
union all
select 'Payment' as tran_type, date(ot_date) tran_date, 'OT Discount' desp, 1 as qty, sum(ifnull(disc_a,0)) amount, 'Total Payment : ' as TRAN_OPTION
FROM ot_his
where deleted = false and admission_no = $P{adm_no}
and date(ot_date) between date($P{adm_date}) and date($P{tran_date})
union all
select 'Payment' as tran_type, date(dc.ot_date) tran_date, 'OT Refund' desp, ddh.qty as qty, (-sum(ifnull(ddh.qty,1) * ifnull(ddh.price,0))) amount, 'Total Payment : ' as TRAN_OPTION
FROM ot_his dc,ot_details_his ddh
where dc.ot_inv_id = ddh.vou_no and ddh.service_id in (select sys_prop_value from sys_prop
								    where sys_prop_desp in ('system.ot.refund.id')) and dc.deleted = false and dc.admission_no = $P{adm_no}
and date(dc.ot_date) between date($P{adm_date}) and date($P{tran_date})
group by date(dc.ot_date) having amount != 0
union all
select 'Payment' as tran_type, date(ot_date) tran_date, 'OT Tax' desp, 1 as qty, sum(ifnull(tax_a,0)) amount, 'Total Payment : ' as TRAN_OPTION
FROM ot_his
where deleted = false and admission_no = $P{adm_no}
and date(ot_date) between date($P{adm_date}) and date($P{tran_date})
group by date(ot_date) having amount > 0
union all
select 'Payment' as tran_type, date(ot_date) tran_date, 'OT Paid' desp, 1 as qty, sum(ifnull(paid,0)) amount, 'Total Payment : ' as TRAN_OPTION
FROM ot_his
where deleted = false and admission_no = $P{adm_no}
and date(ot_date) between date($P{adm_date}) and date($P{tran_date})
group by date(ot_date) having amount > 0
union all
select 'Payment' as tran_type, date(dc_date) tran_date, 'DC Discount' desp, 1 as qty, sum(ifnull(disc_a,0)) amount, 'Total Payment : ' as TRAN_OPTION
FROM dc_his
where deleted = false and admission_no = $P{adm_no}
and date(dc_date) between date($P{adm_date}) and date($P{tran_date})
group by date(dc_date) having amount > 0
union all
select 'Payment' as tran_type, date(dc.dc_date) tran_date, 'DC Refund' desp, ddh.qty as qty, (- sum(ifnull(ddh.qty,1) * ifnull(ddh.price,0))) amount, 'Total Payment : ' as TRAN_OPTION
FROM dc_his dc,dc_details_his ddh
where dc.dc_inv_id = ddh.vou_no and ddh.service_id in (select sys_prop_value from sys_prop
								    where sys_prop_desp in ('system.dc.refund.id')) and dc.deleted = false and dc.admission_no = $P{adm_no}
and date(dc.dc_date) between date($P{adm_date}) and date($P{tran_date})
group by date(dc.dc_date) having amount != 0
union all
select 'Payment' as tran_type, date(dc_date) tran_date, 'DC Tax' desp, 1 as qty, sum(ifnull(tax_a,0)) amount, 'Total Payment : ' as TRAN_OPTION
FROM dc_his
where deleted = false and admission_no = $P{adm_no}
and date(dc_date) between date($P{adm_date}) and date($P{tran_date})
group by date(dc_date) having amount > 0
union all
select tran_type,tran_date,desp,qty,amount,tran_option
from
(select 'Payment' as tran_type, date(dc_date) tran_date, concat('DC ',s.service_name) desp, sum(ddh.qty) qty, sum(ifnull(ddh.amount,0)) amount, 'Total Payment : ' as TRAN_OPTION
FROM dc_his dh,dc_details_his ddh, inp_service s
where deleted = false and admission_no = $P{adm_no} and dh.dc_inv_id = ddh.vou_no and ddh.service_id = s.service_id
and date(dc_date) between date($P{adm_date}) and date($P{tran_date})
and ddh.service_id in (select sys_prop_value from sys_prop
								    where sys_prop_desp in ('system.dc.deposite.id','system.dc.paid.id'))
group by date(dc_date),ddh.service_id having amount > 0)tmp
group by tran_type,tran_date,desp,qty,amount,tran_option ) a
where a.tran_date is not null and a.amount <> 0
group by tran_type, tran_date, desp
) a
where a.tran_date is not null and a.amount <> 0
group by a.tran_date,a.tran_type,a.TRAN_OPTION,a.TRAN_OPTION1,a.desp
order by a.TRAN_OPTION, a.tran_date, a.tran_type, a.desp]]>
	</queryString>

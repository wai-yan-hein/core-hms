	<parameter name="session_user_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select * from
(select oc.cat_name,sum(ifnull(odh.amount,0)) ttl,'OPD' tran_option
from opd_service os,opd_details_his odh,opd_his oh,opd_category oc
where oh.opd_inv_id=odh.vou_no and
odh.service_id=os.service_id and os.cat_id=oc.cat_id 
and date(oh.opd_date) between $P{session_fdate} and $P{session_tdate}
and (oh.session_id = $P{session_id} or '-1' = $P{session_id})
and (oh.currency_id = $P{session_currency} or 'All' = $P{session_currency})
and (oh.payment_id = $P{session_paymentid} or '-1' = $P{session_paymentid})
and (oh.patient_id = $P{pt_id} or '-1' = $P{pt_id})
and (oh.doctor_id = $P{dr_id} or '-1' = $P{dr_id})
and (oh.created_by =  $P{session_user_id} or $P{session_user_id} = 'All')
and (oh.deleted = false)
group by oc.cat_name
Union
select oc.cat_name,sum(ifnull(odh.amount,0)) ttl,'DC' tran_option
from inp_service os,dc_details_his odh,dc_his oh,inp_category oc
where oh.dc_inv_id=odh.vou_no and
odh.service_id=os.service_id and os.cat_id=oc.cat_id 
and date(oh.dc_date) between $P{session_fdate} and $P{session_tdate} 
and (oh.session_id = $P{session_id} or '-1' = $P{session_id}) and (oh.currency_id = $P{session_currency} or 'All' = $P{session_currency})
and (oh.payment_id = $P{session_paymentid} or '-1' = $P{session_paymentid})
and (oh.created_by = $P{session_user_id} or $P{session_user_id} = 'All') and (oh.patient_id = $P{pt_id} or '-1' = $P{pt_id})
and (oh.doctor_id = $P{dr_id} or '-1' = $P{dr_id})
and (oh.deleted = false)
group by oc.cat_name
Union
select oc.group_name,sum(ifnull(odh.amount,0)) ttl,'OT' tran_option
from ot_service os,ot_details_his odh,ot_his oh,ot_group oc
where oh.ot_inv_id=odh.vou_no and
odh.service_id=os.service_id and os.group_id=oc.group_id 
and date(oh.ot_date) between $P{session_fdate} and $P{session_tdate} 
and (oh.session_id = $P{session_id} or '-1' = $P{session_id}) and (oh.currency_id = $P{session_currency} or 'All' = $P{session_currency})
and (oh.payment_id = $P{session_paymentid} or '-1' = $P{session_paymentid})
and (oh.created_by = $P{session_user_id} or $P{session_user_id} = 'All') and (oh.patient_id = $P{pt_id} or '-1' = $P{pt_id})
and (oh.doctor_id = $P{dr_id} or '-1' = $P{dr_id})
and (oh.deleted = false)
group by oc.group_name
) aa
where aa.tran_option = $P{tran_option} or 'All' = $P{tran_option}]]>
	</queryString>

	<parameter name="prm_location" class="java.lang.Integer"/>
	<parameter name="prm_currency" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 	v_return_in.currency 'Return In Currency',
	currency.cur_name 'Currnecy Name',
	v_return_in.med_id 'Medicine Id',
	v_return_in.med_name 'Medicine Name',
	get_qty_in_str(sum(ret_in_smallest_qty),
	v_med_unit_smallest_rel.unit_smallest, v_med_unit_smallest_rel.unit_str) 'Total_Quantity',
	sum(v_return_in.ret_in_amount) 'Amount'
FROM 	v_return_in, currency, v_med_unit_smallest_rel
WHERE 	v_return_in.currency = currency.cur_code
AND 	v_return_in.med_id = v_med_unit_smallest_rel.med_id
AND 	v_return_in.deleted = FALSE
AND 	DATE(v_return_in.ret_in_date) BETWEEN $P{prm_from} AND $P{prm_to}
AND 	(v_return_in.location = $P{prm_location} OR $P{prm_location} = 0)
AND 	(v_return_in.currency = $P{prm_currency} OR $P{prm_currency} = 'All')


GROUP BY 	v_return_in.currency,
	currency.cur_name,
	v_return_in.med_id,
	v_return_in.med_name]]>
	</queryString>
